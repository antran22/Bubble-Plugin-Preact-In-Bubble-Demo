<script src="https://www.unpkg.com/preact@^10.18.0/dist/preact.min.umd.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://www.unpkg.com/preact@^10.18.0/hooks/dist/hooks.umd.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://www.unpkg.com/preact@^10.18.0/compat/dist/compat.umd.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://www.unpkg.com/browse/preact-custom-element@^4.2.1/dist/preact-custom-element.umd.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

<script>
    function loadJS(url, module = false) {
        let scriptEle = document.createElement("script");
        scriptEle.setAttribute("src", url);
        scriptEle.setAttribute("type", module ? "module" : "text/javascript" );
        document.head.appendChild(scriptEle);
    }
    function loadCSS(url) {
        let link = document.createElement("link");
        link.rel  = 'stylesheet';
	    link.type = 'text/css';
    	link.href = url;
	    link.media = 'all';
        document.head.appendChild(link);
    }
    function loadCSSString(css) {
        let style = document.createElement("style");
        style.innerText = css;
        document.head.appendChild(style);
    }

    window.devServerURL = "_*_DEV_SERVER_URL_*_";
</script>